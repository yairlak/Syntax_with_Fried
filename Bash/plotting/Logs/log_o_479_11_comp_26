python plot_evoked_comparison.py --path2figures ../../Figures/Comparisons/26_gender_pronoun2noun_audio/patient_479_11/LSTG/micro -patient 479_11 --micro-macro micro -channel 16 --queries-to-compare Masculine "word_position==1 and word_string=='he' and block in [2, 4, 6]" --queries-to-compare Feminine "word_position==1 and word_string=='she' and block in [2, 4, 6]" --queries-to-compare Masculine "word_position==2 and (word_string=='boy' or word_string=='boys' or word_string=='man' or word_string=='men') and block in [2, 4, 6]" --queries-to-compare Feminine "word_position==2 and (word_string=='girl' or word_string=='girls' or word_string=='woman' or word_string=='women') and block in [2, 4, 6]"
--------------------------------------------------------------------------------
python plot_evoked_comparison.py --path2figures ../../Figures/Comparisons/26_gender_pronoun2noun_audio/patient_479_11/LSTG/macro -patient 479_11 --micro-macro macro -channel 8 --queries-to-compare Masculine "word_position==1 and word_string=='he' and block in [2, 4, 6]" --queries-to-compare Feminine "word_position==1 and word_string=='she' and block in [2, 4, 6]" --queries-to-compare Masculine "word_position==2 and (word_string=='boy' or word_string=='boys' or word_string=='man' or word_string=='men') and block in [2, 4, 6]" --queries-to-compare Feminine "word_position==2 and (word_string=='girl' or word_string=='girls' or word_string=='woman' or word_string=='women') and block in [2, 4, 6]"
--------------------------------------------------------------------------------
