python plot_evoked_comparison.py --path2figures ../../Figures/Comparisons/27_gender_pronoun2noun_visual/patient_502/REC/macro -patient 502 --micro-macro macro -channel 79 --queries-to-compare Masculine "word_position==1 and word_string=='he' and block in [1, 3, 5]" --queries-to-compare Feminine "word_position==1 and word_string=='she' and block in [1, 3, 5]" --queries-to-compare Masculine "word_position==2 and (word_string=='boy' or word_string=='boys' or word_string=='man' or word_string=='men') and block in [1, 3, 5]" --queries-to-compare Feminine "word_position==2 and (word_string=='girl' or word_string=='girls' or word_string=='woman' or word_string=='women') and block in [1, 3, 5]"
--------------------------------------------------------------------------------
python plot_evoked_comparison_rasters.py --path2figures ../../Figures/Comparisons/27_gender_pronoun2noun_visual/patient_502/REC/spike -patient 502 -channel 41 --queries-to-compare Masculine "word_position==1 and word_string=='he' and block in [1, 3, 5]" --queries-to-compare Feminine "word_position==1 and word_string=='she' and block in [1, 3, 5]"
--------------------------------------------------------------------------------
