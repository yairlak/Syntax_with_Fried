--------------------------------------------------------------------------------
python plot_evoked_comparison.py --path2figures ../../Figures/Comparisons/25_gender_nouns_audio2visual/patient_502/RAC/micro -patient 502 --micro-macro micro -channel 89 --queries-to-compare Masculine "word_position==2 and (word_string=='boy' or word_string=='boys' or word_string=='man' or word_string=='men') and block in [2, 4, 6]" --queries-to-compare Feminine "word_position==2 and (word_string=='girl' or word_string=='girls' or word_string=='woman' or word_string=='women') and block in [2, 4, 6]" --queries-to-compare Masculine "word_position==2 and (word_string=='boy' or word_string=='boys' or word_string=='man' or word_string=='men') and block in [1, 3, 5]" --queries-to-compare Feminine "word_position==2 and (word_string=='girl' or word_string=='girls' or word_string=='woman' or word_string=='women') and block in [1, 3, 5]"
--------------------------------------------------------------------------------
