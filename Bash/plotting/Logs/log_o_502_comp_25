--------------------------------------------------------------------------------
python plot_evoked_comparison_rasters.py --path2figures ../../Figures/Comparisons/25_gender_nouns_audio2visual/patient_502/LEC/spike -patient 502 -channel 9 --queries-to-compare Masculine "word_position==2 and (word_string=='boy' or word_string=='boys' or word_string=='man' or word_string=='men') and block in [2, 4, 6]" --queries-to-compare Feminine "word_position==2 and (word_string=='girl' or word_string=='girls' or word_string=='woman' or word_string=='women') and block in [2, 4, 6]"
