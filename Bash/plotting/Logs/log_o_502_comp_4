--------------------------------------------------------------------------------
python plot_evoked_comparison.py --path2figures ../../Figures/Comparisons/4_grammatical_number_nouns_audio2visual/patient_502/LOF/micro -patient 502 --micro-macro micro -channel 87 --queries-to-compare Singular "word_position==2 and (word_string=='boy' or word_string=='girl' or word_string=='man' or word_string=='woman') and block in [2, 4, 6]" --queries-to-compare Plural "word_position==2 and (word_string=='boys' or word_string=='girls' or word_string=='men' or word_string=='women') and block in [2, 4, 6]" --queries-to-compare Singular "word_position==2 and (word_string=='boy' or word_string=='girl' or word_string=='man' or word_string=='woman') and block in [1, 3, 5]" --queries-to-compare Plural "word_position==2 and (word_string=='boys' or word_string=='girls' or word_string=='men' or word_string=='women') and block in [1, 3, 5]"
